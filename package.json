{
  "name": "@alexandernanberg/bricks",
  "description": "A zero-configuration toolkit for building modern web apps",
  "version": "4.1.0",
  "author": "Alexander Nanberg <alexander.nanberg@strateg.se>",
  "license": "MIT",
  "repository": "github:alexandernanberg/bricks",
  "keywords": [
    "bricks",
    "toolkit",
    "0CJS",
    "zero-configuration"
  ],
  "engines": {
    "node": ">=12"
  },
  "files": [
    "babel.js",
    "bin",
    "config",
    "scripts",
    "tasks",
    "utils"
  ],
  "bin": {
    "bricks": "./bin/bricks.js"
  },
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write **/*.js"
